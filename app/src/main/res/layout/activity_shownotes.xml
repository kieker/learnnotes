<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".shownotes">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="420dp"
        android:layout_height="138dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/trumpetpos" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="366dp"
        android:layout_height="234dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.264"
        app:srcCompat="@drawable/c_low" />

    <Button
        android:id="@+id/button10"
        android:layout_width="143dp"
        android:layout_height="44dp"
        android:layout_marginStart="12dp"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="@+id/group"
        app:layout_constraintStart_toEndOf="@+id/checkBox3"
        app:layout_constraintTop_toTopOf="@+id/group" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="152dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="152dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="44dp"
        android:fontFamily="sans-serif-black"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:layout_width="329dp"
        android:layout_height="47dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="66dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/A"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"

            android:onClick="assignNoteName"
            android:text="A"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/B"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="B"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/C"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="C"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/D"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="D"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/E"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="E"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/F"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="F"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

        <RadioButton
            android:id="@+id/G"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@layout/custom_preset_button"
            android:button="@null"
            android:onClick="assignNoteName"
            android:text="G"
            android:textAlignment="center"
            android:textColor="@layout/button_color"
            tools:layout_editor_absoluteX="23dp"
            tools:layout_editor_absoluteY="75dp" />

    </RadioGroup>

    <CheckBox
        android:id="@+id/checkBox0"
        android:layout_width="45dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:background="@layout/custom_preset_button"
        android:button="@null"
        android:onClick="noteSelect"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@layout/button_color"
        app:layout_constraintBottom_toBottomOf="@+id/group"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/group" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="45dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:background="@layout/custom_preset_button"
        android:button="@null"
        android:onClick="noteDeselect"
        android:text="1"
        android:textAlignment="center"
        android:textColor="@layout/button_color"
        app:layout_constraintBottom_toBottomOf="@+id/group"
        app:layout_constraintStart_toEndOf="@+id/checkBox0"
        app:layout_constraintTop_toTopOf="@+id/group" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="45dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:background="@layout/custom_preset_button"
        android:button="@null"
        android:onClick="noteDeselect"
        android:text="2"
        android:textAlignment="center"
        android:textColor="@layout/button_color"
        app:layout_constraintBottom_toBottomOf="@+id/group"
        app:layout_constraintStart_toEndOf="@+id/checkBox1"
        app:layout_constraintTop_toTopOf="@+id/group" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="45dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:background="@layout/custom_preset_button"
        android:button="@null"
        android:onClick="noteDeselect"
        android:text="3"
        android:textAlignment="center"
        android:textColor="@layout/button_color"
        app:layout_constraintBottom_toBottomOf="@+id/group"
        app:layout_constraintStart_toEndOf="@+id/checkBox2"
        app:layout_constraintTop_toTopOf="@+id/group" />

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="366dp"
        android:layout_height="192dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="160dp"
        app:constraint_referenced_ids="checkBox0,checkBox1,checkBox2,checkBox3,button10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>